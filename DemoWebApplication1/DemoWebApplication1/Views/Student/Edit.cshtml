@model DemoWebApplication1.Models.student
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
</head>
<body>
    <div>
        @using (Html.BeginForm())
        {
            @Html.ActionLink("got to Index", "Index");
            <table border="1">
                <tr>
                    <th>Roll No</th>
                    <th>@Html.TextBoxFor(e => e.rno)</th>
                </tr>
                <tr>
                    <th> Name</th>
                    <th>@Html.TextBoxFor(e => e.name)</th>
                </tr>
                <tr>
                    <th> Qualification</th>
                    <th>@Html.TextBoxFor(e => e.qualification)</th>
                </tr>
                <tr>
                    <th> Percentage</th>
                    <th>@Html.TextBoxFor(e => e.percentage)</th>
                </tr>

                <tr>
                    <th><input type ="submit" value="submit" /></th>
                </tr>
            </table>
         

        }
    </div>
